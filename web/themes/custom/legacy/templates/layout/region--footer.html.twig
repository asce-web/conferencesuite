{#
/**
 * @file
 * Theme override to display a region.
 *
 * Available variables:
 * - content: The content for this region, typically blocks.
 * - attributes: HTML attributes for the region div.
 * - region: The name of the region variable as defined in the theme's
 *   .info.yml file.
 *
 * @see template_preprocess_region()
 */
#}
{{ attach_library('legacy/footerpanels') }}
{% if content %}
  <div{{ attributes.addClass('o-Tablist o-Tablist--footertabs') }} role="tablist">
      <!--
        NOTE: for accessibility, the order of blocks should be:
        1. current year
        2. previous year
        3. next year
      -->
      {{ content }}
  </div>
{% endif %}
