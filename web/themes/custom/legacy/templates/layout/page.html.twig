{{ attach_library('legacy/header') }}
{{ attach_library('legacy/copyright') }}
<header>
  <nav class="o-Flex o-MobileBar c-Banner c-Banner--nav -d-n-sM">
    <h1 class="h-Hidden">Mobile Menu</h1>
    <a class="o-Flex__Item o-MobileBar__Item h-Block" href="/">
      <i class="glyphicons glyphicons-home"><span class="h-Hidden">Home</span></i>
    </a>
    <label class="o-Flex__Item o-MobileBar__Item c-Toggle h-Block">
      <input class="c-Toggle__Check h-Hidden" type="checkbox" value="main menu is open" role="switch"/>
      <i class="c-Toggle__Viz glyphicons glyphicons-menu-hamburger">
        <span class="h-Hidden">toggle main menu</span>
      </i>
    </label>
    <noscript><a href="#sitemap">Site Map</a></noscript>
    {{ page.menu_mobile }}
  </nav>
  <div class="c-Banner c-Banner--head">
    <div class="o-Flex o-Constrain">
      <a class="o-Flex__Item o-MainCol h-Block" href="{{ path('<front>') }}" rel="home">
        {{ page.header }}
      </a>
      <div class="o-Flex__Item o-SideCol c-TopImg -d-n-nM" style="background-image: url('{{ page_images_banner }}');">
        <span class="c-YearBadge" role="none">{{ (page_startdate|date('Y'))[2:] }}</span>
        {{ page.menu_contrib }}
      </div>
    </div>
  </div>
  <nav class="c-Banner c-Banner--nav -d-n-nM">
    <h1 class="h-Hidden">Main Menus</h1>
    <div class="o-Constrain o-Flex">
      <div class="o-Flex__Item o-MainCol">
        {{ page.menu_main }}
      </div>
      <div class="o-Flex__Item o-SideCol">
        {{ page.menu_register }}
      </div>
    </div>
  </nav>
  <nav>
    <h1 class="h-Hidden">Sub Menus</h1>
      {{ page.menu_sub }}
  </nav>
</header>
<main class="o-Constrain o-Flex -xd-c-nM -mb-2" id="main">
    <div class="o-Flex__Item o-MainCol">{{ page.content }}</div>
    <div class="o-Flex__Item o-SideCol -pt-1 -ph-2">{{ page.sidebar }}</div>
</main>
<aside>
  <div class="o-Constrain">
    <h1 class="h-Hidden">Supporters</h1>
    {{ page.supporters }}
  </div>
</aside>
<footer>
  <fieldset>{{ page.footer }}</fieldset>
  <p class="c-Banner c-Banner--copyright -mb-z -ta-c">
