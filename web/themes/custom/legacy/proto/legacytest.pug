doctype html
html(lang="en")
  -
    var conference = {
      name     : 'Sample ASCE Conference',
      classname: '',
      menuhover: false,
      secondary_logo: false,
      tagline  : 'Optional Tagline for the Conference Goes Here Lorem Ipsum',
      theme    : 'New ideas. New practices. New science. New resources. New colleagues. New business procedures.',
      detail   : 'Boston, Massachusetts  |  April 3–5, 2015',
      image    : {
        shield : '../logo.svg',
        hero   : '../../../../sites/default/files/2017-05/new-orleans-trolley.jpg',
        banner : '../../../../sites/default/files/site-images/CECAR%207%20Banner%20Image.jpg',
        sidebar: '../../../../sites/default/files/2017-07/slide_2.jpg',
      },
      colors: {
        primary  : '#6a9556',
        secondary: '#6fc2c4',
        accent   : '#714509',
      }
    }
  head
    title Sample ASCE Conference
    meta(charset="utf-8")
    meta(name="description" content="")
    meta(name="viewport" content="width=device-width, initial-scale=1")
    meta(http-equiv="X-UA-Compatible" content="IE=edge,chrome=1")
    link(rel="stylesheet" href="//fonts.googleapis.com/css?family=Open+Sans:300,400italic,600italic,700italic,400,600,700")
    link(rel="stylesheet" href="../node_modules/xmeter/css/xmeter.css")
    link(rel="stylesheet" href="https://cdn.rawgit.com/chharvey/bangs/v0.15.0/bangs.css")
    link(rel="stylesheet" href="../vendor/futura.css")
    link(rel="stylesheet" href="../vendor/glyphicons_pro_1_9_2/glyphicons/web/html_css/css/glyphicons.css")
    link(rel="stylesheet" href="../vendor/glyphicons_pro_1_9_2/glyphicons-filetypes/web/html_css/css/glyphicons-filetypes.css")
    link(rel="stylesheet" href="../vendor/glyphicons_pro_1_9_2/glyphicons-halflings/web/html_css/css/glyphicons-halflings.css")
    link(rel="stylesheet" href="../vendor/glyphicons_pro_1_9_2/glyphicons-social/web/html_css/css/glyphicons-social.css")
    link(rel="stylesheet" href="../css/legacy.css")
    style.
      body {
        --color-primary:   #{conference.colors.primary};
        --color-secondary: #{conference.colors.secondary};
        --color-accent:    #{conference.colors.accent};
      }
    noscript
      // remove Toggle menu when JS is disabled
      style.
        @media not all and (min-width: 45em) {
          .c-Toggle,
          .o-Drawer { display: none; }
        }
  body#top(class=conference.classname)
    a.h-Hidden(href="#main") skip to main content
    header
