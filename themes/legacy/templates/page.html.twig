<div class="">
  <!--[if lt IE 7]> <p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p> <![endif]-->
  <div class="toggleable-nav fade">
    {{ page.mobile_menu }}
  </div>
  <header class="header">
    <div class="visible-xs mobile-nav navigation">
      <a href="/" aria-label="home"><span class="ic home"></span></a>
      <a href="" class="menu-toggle" aria-label="open menu"><span class="ic menu"></span></a>
    </div>
    <div class="c-Mast">
      <div class="o-Flex o-Constrain">
        <a class="o-Flex__Item o-Flex o-MainCol o-HomeLink" href="{{ path('<front>') }}" rel="home">
          {{ page.site_branding }}
        </a>
        <div class="o-Flex__Item o-SideCol c-TopImg">
          <span class="c-YearBadge">{{ (page_current_conference_start_date|date('Y'))[2:] }}</span>
          <div class="o-Flex c-NavBar c-NavBar--contrib">
            <!-- change this div to a ul.o-List later -->
            {{ page.contributors_menu }}
          </div>
        </div>
      </div>
    </div>
    <nav class="c-Mast c-Mast--nav">
      <div class="o-Flex o-Constrain">
        <div class="o-Flex__Item o-Flex o-MainCol c-NavBar c-NavBar--main">
          <!-- change this div to a ul.o-List later -->
          {{ page.main_menu1 }}
        </div>
        <div class="o-Flex__Item o-Flex o-SideCol c-NavBar c-NavBar--register">
          <!-- change this div to a ul.o-List later -->
          {{ page.registration_menu }}
        </div>
      </div>
    </nav>
    <nav class="c-Mast c-Mast--nav c-Mast--nav--sub">
      <div class="o-Constrain">
        {{ page.main_menu2 }}
      </div>
    </nav>
  </header>
  <main class="o-Flex o-Constrain -xd-c-nM">
    <div class="o-Flex__Item o-MainCol">
      {{ page.hero }}
      <section class="h-Spaced">
        {{ page.content }}
      </section>
    </div>
    <div class="o-Flex__Item o-SideCol -ph-2">{{ page.sidebar }}</div>
  </main>
  <div>
    <aside>
      <div class="o-Constrain">
        <h2>Top Sponsors</h2>
        {{ page.sponsors }}
      </div>
    </aside>
    <aside class="c-Mast c-Mast--coop">
      <div class="o-Constrain">
        <h2 class="h-Hidden">Supporting Organizations</h2>
        {{ page.nonsponsors }}
      </div>
    </aside>
  </div>
  <footer class="footer">
    <dl class="o-Flex o-Tabs">
      <!-- NOTE: 'current' displayed first for accessibility -->
      <dt class="o-Flex__Item o-Tabs__Tab" id="tab-current">
        <label class="h-Block">
          <input class="o-Tabs__Check h-Hidden" type="radio" name="footertabs" value="{{ page_current_startdate|date('Y') }}" checked=""/>
          {{ page_current_startdate|date('Y') }}
        </label>
      </dt>
      <dd class="o-Flex__Item o-Tabs__Panel c-FooterPanel" id="panel-current">
        <div class="o-Flex">
          <div class="o-Flex__Item">
            <span class="c-YearBadge">{{ (page_current_startdate|date('Y'))[2:] }}</span>
          </div>
          <div class="o-Flex__Item">
            <h1>{{ page_current_name }}</h1>
          </div>
        </div>
        <div class="o-Flex">
          {% if site_logo %}
            <div class="o-Flex__Item o-Flex__Item--siteLogo">
              <img class="c-ConfLogo" src="{{ site_logo }}" alt="Home"/>
            </div>
          {% endif %}
          {% if site_second_logo %}
            <div class="o-Flex__Item o-Flex__Item--siteLogo">
              <img class="c-ConfLogo" src="{{ site_second_logo }}" alt="Home"/>
            </div>
          {% endif %}
          <div class="o-Flex__Item">
            {{ page.footer }}
          </div>
        </div>
      </dd>
      <dt class="o-Flex__Item o-Tabs__Tab [ js-xo-_1 ]" id="tab-prev">
        <label class="h-Block">
          <input class="o-Tabs__Check h-Hidden" type="radio" name="footertabs" value="{{ page_prev_startdate|date('Y') }}"/>
          {{ page_prev_startdate|date('Y') }}
        </label>
      </dt>
      <dd class="o-Flex__Item o-Tabs__Panel c-FooterPanel" id="panel-prev">
        <div class="o-Flex">
          <div class="o-Flex__Item">
            <span class="c-YearBadge">{{ (page_prev_startdate|date('Y'))[2:] }}</span>
          </div>
          <div class="o-Flex__Item">
            <h1>{{ page_prev_name }}</h1>
          </div>
        </div>
        <div class="o-Flex">
          {% if site_logo %}
            <div class="o-Flex__Item o-Flex__Item--siteLogo">
              <img class="c-ConfLogo" src="{{ site_logo }}" alt="Home"/>
            </div>
          {% endif %}
          {% if site_second_logo %}
            <div class="o-Flex__Item o-Flex__Item--siteLogo">
              <img class="c-ConfLogo" src="{{ site_second_logo }}" alt="Home"/>
            </div>
          {% endif %}
          <div class="o-Flex__Item">
            <p class="c-ConferenceLocation">
              {{ block_prev_location_advisory }}
              |
              <time datetime="{{ block_prev_startdate|date('c') }}">
                {{ block_prev_startdate|date('M d') }}
                {% if block_prev_startdate|date('Y') != block_prev_enddate|date('Y') %}
                  , {{ block_prev_startdate|date('Y') }}
                {% endif %}
              </time>
              &ndash;
              <time datetime="{{ block_prev_startdate|date('c') }}">
                {% if block_prev_startdate|date('M') != block_prev_enddate|date('M') %}
                  {{ block_prev_enddate|date('M') }}
                {% endif %}
                {{ block_prev_enddate|date('d, Y') }}
              </time>
            </p>
            {% if page_prev_theme %}
              <p class="c-ConferenceTagline">{{ page_prev_theme }}</p>
            {% endif %}
          </div>
        </div>
      </dd>
      <dt class="o-Flex__Item o-Tabs__Tab" id="tab-next">
        <label class="h-Block">
          <input class="o-Tabs__Check h-Hidden" type="radio" name="footertabs" value="{{ page_next_startdate|date('Y') }}"/>
          {{ page_next_startdate|date('Y') }}
        </label>
      </dt>
      <dd class="o-Flex__Item o-Tabs__Panel c-FooterPanel" id="panel-next">
        <div class="o-Flex">
          <div class="o-Flex__Item">
            <span class="c-YearBadge">{{ (page_next_startdate|date('Y'))[2:] }}</span>
          </div>
          <div class="o-Flex__Item">
            <h1>{{ page_next_name }}</h1>
          </div>
        </div>
        <div class="o-Flex">
          {% if site_second_logo %}
            <div class="o-Flex__Item o-Flex__Item--siteLogo">
              <img class="c-ConfLogo" src="{{ site_second_logo }}" alt="Home"/>
            </div>
          {% endif %}
          {% if second_logo %}
            <div class="o-Flex__Item o-Flex__Item--siteLogo">
              <img class="c-ConfLogo" src="{{ second_logo }}" alt="Home"/>
            </div>
          {% endif %}
          <div class="o-Flex__Item">
            <p class="c-ConferenceLocation">
              {{ block_next_location_advisory }}
              |
              <time datetime="{{ block_next_startdate|date('c') }}">
                {{ block_next_startdate|date('M d') }}
                {% if block_next_startdate|date('Y') != block_next_enddate|date('Y') %}
                  , {{ block_next_startdate|date('Y') }}
                {% endif %}
              </time>
              &ndash;
              <time datetime="{{ block_next_startdate|date('c') }}">
                {% if block_next_startdate|date('M') != block_next_enddate|date('M') %}
                  {{ block_next_enddate|date('M') }}
                {% endif %}
                {{ block_next_enddate|date('d, Y') }}
              </time>
            </p>
            {% if page_next_theme %}
              <p class="c-ConferenceTagline">{{ page_next_theme }}</p>
            {% endif %}
          </div>
        </div>
      </dd>
    </dl>
  </footer>
  <div id="footer" class="footer container-fluid">
    <div class="visible-xs mobile-nav navigation">
      <a href="/" aria-label="home"><span class="ic home"></span></a>
      <a href="" class="menu-toggle" aria-label="open menu"><span class="ic menu"></span></a>
    </div>
    <div class="row copyright text-center">
      <div class="container">
        <div class="asce-logo"></div><span>Copyright Â© 1996&ndash;{{ now|date("Y") }}, American Society of Civil Engineers | <a href="http://www.asce.org/" target="_blank" title="ASCE">ASCE.org</a></span>
      </div>
    </div>
  </div>
</div>
